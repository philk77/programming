package com.philip.hashmap;

import java.util.HashMap;

public class HashMapEx {

	public HashMap<Integer, Student> buildHashMap (Student[] students) {
		HashMap myHashMap = new HashMap<Integer, Student> ();
		Integer i = 0;
		for (Student s: students) {
			myHashMap.put(i++, s);
		}
		
		return myHashMap;
	}
	
}
